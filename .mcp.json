{
  "mcpServers": {
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "supabase": {
      "type": "http",
      "url": "http://127.0.0.1:54421/mcp"
    },
    "Sentry": {
      "type": "http",
      "url": "https://mcp.sentry.dev/mcp/icelook/javascript-nextjs"
    },
    "mixpanel": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.mixpanel.com/mcp",
        "--allow-http"
      ]
    }
  }
}
